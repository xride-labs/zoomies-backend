{
  "name": "zoomies-backend",
  "version": "1.0.0",
  "description": "Zoomies Backend API with Express, Prisma, and Better Auth.js",
  "type": "module",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npm run prisma:generate && tsc ",
    "start": "node dist/src/server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "auth.js",
    "nextauth"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@auth/express": "^0.7.0",
    "@auth/prisma-adapter": "^2.0.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^6.19.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "mongoose": "^8.0.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.18.0",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^4.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.30",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.22",
    "@types/pg": "^8.16.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "prisma": "^6.19.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.2"
  }
